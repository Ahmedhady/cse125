#include <iostream>
using namespace std;
int main ()
{
    float l1,w1,l2,w2,a1,a2,t;
    cout<<"enter the dimensions"<<endl;
    cin>>l1>>w1>>l2>>w2;
    a1=l1*w1;
    a2=l2*w2;
    t=(a1-a2)/(3.2*60);
    cout <<t<<endl;
    return 0;
}


#include <iostream>
using namespace std;
int main ()
{
 short code;
 cout<< "enter your character code"<<endl;
 cin >>code;
 char ch= code;
 cout <<"the character whose code is "<<code<<"is"<<ch<<endl;
 return 0;
}

