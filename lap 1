#include<iostream>
using namespace std;
int main ()
{
    int x,y,sum,diff,product,quo,remainder;
    cout<<"enter your two numbers"<<endl;
    cin >>x>>y;
    sum=x+y;
    diff=x-y;
    product=x*y;
    quo=x/y;
    remainder=x%y;
    cout <<sum<<endl<<diff<<endl<<product<<endl<<quo<<endl<<remainder<<endl;
    return 0;
}
